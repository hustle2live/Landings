    "sass": "^1.80.3",
    "vite": "^5.4.8"
  },
  "dependencies": {
    "vanilla-framework": "^4.17.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
